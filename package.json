{
  "name": "sar-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --host 0.0.0.0",
    "dev": "npm install && npm run serve",
    "build": "vue-cli-service build",
    "http-server": "http-server dist -i -p 8080",
    "install-ais": "cd services/ais && npm install --no-save",
    "install-boostrap": "cd services/bootstrap && npm install --no-save",
    "install-database": "cd services/database && npm install --no-save",
    "install-location": "cd services/location && npm install --no-save",
    "postinstall": "npm run -s install-ais && npm run -s install-boostrap && npm run -s install-database && npm run -s install-location",
    "lint": "vue-cli-service lint",
    "maptiles:clean": "rimraf dist/MapQuest",
    "maptiles:extract": "extract-zip data/MapTiles.zip $(pwd)/public/"
  },
  "dependencies": {
    "element-ui": "^2.4.11",
    "http-server": "0.9.0",
    "leaflet": "^1.3.4",
    "pouchdb-authentication": "^1.1.3",
    "pouchdb-browser": "^7.0.0",
    "pouchwrapper": "0.0.4",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-service": "^3.8.4",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "extract-zip": "^1.6.7",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "vue/require-v-for-key": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "globals": {
      "L": true
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
